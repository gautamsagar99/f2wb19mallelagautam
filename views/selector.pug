extends layout

block content
  h1= title
  - let image_names=["Ash.jpg","Aspen.jpg","Aspen1.jpg","Birch.jpg","Birch1.jpg"];
  - for(let i=0;i<3;i++) {
      - let randomNumber = Math.floor(Math.random()*5);
      img(src=`/images/${image_names[randomNumber]}` alt=`Image ${randomNumber}` height="300px" width="300px")
  - }

